    "def EuclidAlg(a,b):\n",
    "    if a == 0:\n",
    "        return 0, 1\n",
    "    u, v = extendedEuclideanAlgorithm(b % a, a)\n",
    "    x = v - (b // a ) * u\n",
    "    y = u\n",
    "    return x, y\n",
    "a=int(input(\"Enter first number: \"))\n",
    "b=int(input(\"Enter second number: \"))\n",
    "print(EuclidAlg(a,b))"
